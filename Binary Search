DECLARE MyList: ARRAY[1:10] OF INTEGER
MyList ‚Üê [14,18,20,31,64,73,96,101,102,104]
DECLARE low, high, mid, search : INTEGER
DECLARE found: BOOLEAN 
found <-- FALSE
low <-- 1
high <-- 10

OUTPUT "Enter a value you want to search"
INPUT search

WHILE found = FALSE AND low <= high DO
  mid <-- (low + high) DIV 2
  IF search = MyList[mid]
  THEN found <-- TRUE
  ELSE
      IF search > MyList[mid]
      THEN low <-- mid + 1
      ELSE
          high <-- mid - 1
      ENDIF
  ENDIF
ENDWHILE
IF found
THEN OUTPUT "Value found"
ELSE OUTPUT "Value not found"
ENDIF
